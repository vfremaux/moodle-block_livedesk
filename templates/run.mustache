{{!
    @template block-livedesk/run

    Block template for the color mapping form part.

    Classes required for JS:
    * none

}}

<div class="livedesk-headerlogo" ><img src="{{{logoiconurl}}}" /></div>

<div id="livedesk-toolbar-wrap">
    <div id="livedesk-maintoolbar"></div>
    <div id="livedesk-toolbar"></div>

    <div id="masterlayout">
        <div id="livedesk-masterlayout">
        <div id="livedesk-postsgrid" class="livedesk-panel"></div>
        <div id="livedesk-onlineuserscount" class="livedesk-panel"></div>
        <div id="livedesk-plugins" class="livedesk-panel"></div>

    <div id="livedesk-info">
        <b>{{{name}}}</b> {{{managecmd}}}<br/>
        {{{description}}}
    </div>
</div>

<p align="center">
    <input type="button" value="{{getasexcelstr}}" onclick="mygrid.toExcel('{{wwwroot}}/blocks/livedesk/js/dhtmlx/3.0/grid_exporter/generate.php');">
</p>
